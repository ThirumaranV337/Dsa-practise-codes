class Solution:
    def fourSum(self, arr, target):
        arr.sort()#reduce the time more 
        length=len(arr)
        returning_list=[]#empty list to store the quartile 
        hash_map=set()#creating hash table to easy access
        for i in range(length):
            for j in range(i+1,length):
                for k in range(j+1,length):
                    sum=arr[i]+arr[j]+arr[k]
                    element_finder=target-sum#using the algebric rule
                    if element_finder in hash_map:
                        appending_list=[arr[i],arr[j],arr[k],element_finder]
                        appending_list.sort()
                        if appending_list not in returning_list:
                            
                          returning_list.append(appending_list)
                    hash_map.add(arr[k])
                hash_map.clear()    
       
    
        
        
               
        return  returning_list
object=Solution()
arr=[0, 0, 2, 1, 1]
target=3
ans=object.fourSum(arr,target)
#time complexity o(n^3)
#space complexity o(
        
