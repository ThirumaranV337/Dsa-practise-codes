class Solution:
	def twoSum(self, arr, target):
	    arr_length=len(arr)#finding the length to run the for loop correctly
	    
		for pointer_1 in range(arr_length):#pointer1 to traverse a full array
		    for pointer_2 in range(pointer_1+1,arr_length):#pointer_1+1 was give because the same element was added and give the target example:0 ,so I skipped a same element 
		        if arr[pointer_1]+arr[pointer_2] == target:#camparing the values to find the target 
		            return True
		return False      
